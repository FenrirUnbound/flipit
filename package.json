{
  "name": "flipit",
  "version": "0.4.6",
  "description": "Feature flagging for NodeJS",
  "main": "index.js",
  "scripts": {
    "test": "ytestrunner --coverage --istanbul cover --cov-include=index.js --cov-include=lib/* --cov-exclude=**/node_modules/** --save-coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/FenrirUnbound/flipit.git"
  },
  "author": "Darren m",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FenrirUnbound/flipit/issues"
  },
  "devDependencies": {
    "jslint": "*",
    "mockery": "*",
    "yuitest": "*",
    "ytestrunner": "*",
    "coveralls": "~2.6.1"
  },
  "dependencies": {
    "path": "~0.4.9"
  }
}
