{
  "name": "flipit",
  "version": "2.0.3",
  "description": "Feature flagging for NodeJS",
  "main": "index.js",
  "scripts": {
    "test": "ytestrunner --exclude=tests/**/*.js --coverage --istanbul cover --cov-include=index.js --cov-include=lib/* --cov-exclude=**/node_modules/** --cov-exclude=**/unit/** --save-coverage",
    "try": "mocha ./tests --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/FenrirUnbound/flipit.git"
  },
  "keywords": [
    "feature",
    "features",
    "flag",
    "flags",
    "flagging",
    "toggle",
    "switch",
    "switches",
    "feature flagging",
    "feature toggling",
    "feature switching",
    "continuous delivery",
    "continuous deployment"
  ],
  "author": "Darren M",
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "bugs": {
    "url": "https://github.com/FenrirUnbound/flipit/issues"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "coveralls": "~2.6.1",
    "jslint": "*",
    "mocha": "^2.2.5",
    "mockery": "*",
    "sinon": "^1.15.3",
    "sinon-chai": "^2.8.0",
    "ytestrunner": "*",
    "yuitest": "*"
  }
}
